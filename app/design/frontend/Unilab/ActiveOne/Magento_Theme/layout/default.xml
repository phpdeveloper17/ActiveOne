         <referenceBlock name="product.attributes" remove="true" />
           <referenceBlock name="product_viewed_counter" remove="true" />
           
            <referenceBlock name="product.info.review" remove="true" />
 -->

 <referenceBlock name="catalog.compare.sidebar" remove="true"/>


        <referenceContainer name="footer-container"> 
            <container name="footer" as="footer" label="Page Footer" htmlTag="div" htmlClass="footer">
                <container name="footer-wrapper" as="footer.wrapper" label="Prapper Footer Blocks" htmlTag="div" htmlClass="row-footer">
                    <block class="Magento\Framework\View\Element\Template" name="footer.custom" as="footer_custom" template="Magento_Theme::html/footer_links.phtml"/>
                  </container>
            </container>
        </referenceContainer>    
</body>

</page>


