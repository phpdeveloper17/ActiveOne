<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
	<group id="default">
		<job instance="Unilab\Benefits\Cron\Autorefreshperiod" method="execute" name="refresh_benefits">
			<schedule>*/10 * * * *</schedule><!-- 10 minute-->
		</job>
	</group>
</config>
