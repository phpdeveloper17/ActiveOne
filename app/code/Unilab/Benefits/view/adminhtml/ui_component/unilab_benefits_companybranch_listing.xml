<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing_data_source</item>
            <item name="deps" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing_data_source</item>
        </item>
        <item name="spinner" xsi:type="string">spinner_columns</item>
        <item name="buttons" xsi:type="array">
            <item name="add" xsi:type="array">
                <item name="name" xsi:type="string">add</item>
                <item name="label" xsi:type="string" translate="true">Add New Company Address</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/create</item>
            </item>
            <item name="import" xsi:type="array">
                <item name="name" xsi:type="string">import</item>
                <item name="label" xsi:type="string" translate="true">Import Company Address</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/import</item>
            </item>
        </item>
    </argument>
    <dataSource name="nameOfDataSource">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider</argument>
            <argument name="name" xsi:type="string">unilab_benefits_companybranch_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">id</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>

    <!-- Start Toolbar -->
    <container name="listing_top">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="template" xsi:type="string">ui/grid/toolbar</item>
                <item name="stickyTmpl" xsi:type="string">ui/grid/sticky/toolbar</item>
            </item>
        </argument>
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filters name="listing_filters">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="columnsProvider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.spinner_columns</item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.filters</item>
                    </item>
                    <item name="templates" xsi:type="array">
                        <item name="filters" xsi:type="array">
                            <item name="select" xsi:type="array">
                                <item name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</item>
                                <item name="template" xsi:type="string">ui/grid/filters/elements/ui-select</item>
                            </item>
                        </item>
                    </item>
                    <item name="childDefaults" xsi:type="array">
                        <item name="provider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.listing_top.listing_filters</item>
                        <item name="imports" xsi:type="array">
                            <item name="visible" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.spinner_columns.${ $.index }:visible</item>
                        </item>
                    </item>
                </item>
                <item name="observers" xsi:type="array">
                    <item name="column" xsi:type="string">column</item>
                </item>
            </argument>
        </filters>

        <!-- Filter Search -->
        <!-- <filterSearch name="fulltext">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="provider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing_data_source</item>
                    <item name="chipsProvider" xsi:type="string">jobs_department_listing.jobs_department_listiunilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.listing_top.listing_filters_chips</item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.search</item>
                    </item>
                </item>
            </argument>
        </filterSearch> -->

        <!-- Full Text Search -->
        <!-- <filterSearch name="fulltext"/> -->
        
        <massaction name="listing_massaction">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="selectProvider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.spinner_columns.ids</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/tree-massactions</item>
                    <item name="indexField" xsi:type="string">id</item>
                </item>
            </argument>
            <!-- Mass actions which you want to add in your grid-->
            <action name="delete">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">delete</item>
                        <item name="label" xsi:type="string" translate="true">Delete</item>
                        <item name="url" xsi:type="url" path="unilab_benefits/CompanyBranchController/massdelete"/>
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Delete</item>
                            <item name="message" xsi:type="string" translate="true">Do you want to delete selected row record?</item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
        <paging name="listing_paging">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.paging</item>
                    </item>
                    <item name="selectProvider" xsi:type="string">unilab_benefits_companybranch_listing.unilab_benefits_companybranch_listing.spinner_columns.ids</item>
                </item>
            </argument>
        </paging>
    </container>
    <!-- End Toolbar -->

    <columns name="spinner_columns">
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="string">55</item>
                    <item name="indexField" xsi:type="string">id</item>
                </item>
            </argument>
        </selectionsColumn>
        <column name="id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">10</item>
                    <item name="filter" xsi:type="string">textRange</item>
                    <item name="sorting" xsi:type="string">asc</item>
                    <item name="label" xsi:type="string" translate="true">ID</item>
                </item>
            </argument>
        </column>
        <column name="company_id" component="Magento_Ui/js/grid/columns/select">
           <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Unilab\Benefits\Model\Source\Company</item>
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">20</item>
                   <item name="filter" xsi:type="string">select</item>
                   <item name="dataType" xsi:type="string">select</item>
                   <item name="editor" xsi:type="string">select</item>
                   <item name="label" xsi:type="string" translate="true">Company Name</item>
               </item>
           </argument>
       </column>

       <column name="contact_number" >
           <argument name="data" xsi:type="array">
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">30</item>
                   <item name="filter" xsi:type="string">false</item>
                   <item name="label" xsi:type="string" translate="true">Contact Number</item>
               </item>
           </argument>
       </column>

       <column name="contact_person" >
           <argument name="data" xsi:type="array">
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">40</item>
                   <item name="filter" xsi:type="string">false</item>
                   <item name="label" xsi:type="string" translate="true">Contact Person</item>
               </item>
           </argument>
       </column>

       <column name="branch_address" >
           <argument name="data" xsi:type="array">
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">50</item>
                   <item name="filter" xsi:type="string">false</item>
                   <item name="label" xsi:type="string" translate="true">Address</item>
               </item>
           </argument>
       </column>

       <column name="branch_city" >
           <argument name="data" xsi:type="array">
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">60</item>
                   <item name="filter" xsi:type="string">false</item>
                   <item name="label" xsi:type="string" translate="true">City</item>
               </item>
           </argument>
       </column>

       <column name="branch_province" component="Magento_Ui/js/grid/columns/select">
           <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Unilab\Benefits\Model\Source\Province</item>
               <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">select</item>
                   <item name="dataType" xsi:type="string">select</item>
                    <item name="sortOrder" xsi:type="number">70</item>
                   <item name="label" xsi:type="string" translate="true">State/Province</item>
               </item>
           </argument>
       </column>

       <column name="branch_postcode" >
           <argument name="data" xsi:type="array">
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">80</item>
                   <item name="filter" xsi:type="string">false</item>
                   <item name="label" xsi:type="string" translate="true">Postal Code</item>
               </item>
           </argument>
       </column>

       <column name="ship_code" >
           <argument name="data" xsi:type="array">
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">90</item>
                   <item name="filter" xsi:type="string">false</item>
                   <item name="label" xsi:type="string" translate="true">Ship Code</item>
               </item>
           </argument>
       </column>

       <column name="shipping_address" component="Magento_Ui/js/grid/columns/select">
           <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Unilab\Benefits\Model\Source\ShippingStatus</item>
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">100</item>
                   <item name="filter" xsi:type="string">select</item>
                   <item name="dataType" xsi:type="string">select</item>
                   <item name="editor" xsi:type="string">select</item>
                   <item name="label" xsi:type="string" translate="true">Is Shipping Add.</item>
               </item>
           </argument>
       </column>

       <column name="billing_address" component="Magento_Ui/js/grid/columns/select">
           <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Unilab\Benefits\Model\Source\BillingStatus</item>
               <item name="config" xsi:type="array">
                   <item name="sortOrder" xsi:type="number">110</item>
                   <item name="filter" xsi:type="string">select</item>
                   <item name="dataType" xsi:type="string">select</item>
                   <item name="editor" xsi:type="string">select</item>
                   <item name="label" xsi:type="string" translate="true">Is Billing Add.</item>
               </item>
           </argument>
       </column>

       <column name="created_time" class="Magento\Ui\Component\Listing\Columns\Date">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">120</item>
                    <item name="filter" xsi:type="string">dateRange</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
                    <item name="dataType" xsi:type="string">date</item>
                    <item name="label" xsi:type="string" translate="true">Created Date</item>
                </item>
            </argument>
        </column>
        <column name="update_time" class="Magento\Ui\Component\Listing\Columns\Date">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">130</item>
                    <item name="filter" xsi:type="string">dateRange</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
                    <item name="dataType" xsi:type="string">date</item>
                    <item name="label" xsi:type="string" translate="true">Modified Date</item>
                </item>
            </argument>
        </column>
        <column name="webstore_id" class="Unilab\Webservice\Model\Source\Storeviews">
            <settings>
                <!-- <addField>true</addField> -->
                <options class="Magento\Config\Model\Config\Source\Store"/>
                <filter>select</filter>
                <dataType>select</dataType>
                <label translate="true">Store View</label>
                <bodyTmpl>ui/grid/cells/html</bodyTmpl>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="website_id" class="Unilab\Webservice\Model\Source\Website">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Website</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">110</item>
                    <item name="filter" xsi:type="string">select</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="editor" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Website</item>
                </item>
            </argument>
        </column>
        <!-- Edit Action -->
        <actionsColumn 
            name="actions" 
            class="Unilab\Benefits\Ui\Component\Listing\CompanyBranch\Column\Action">
           <argument name="data" xsi:type="array">
               <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">140</item>
                   <item name="resizeEnabled" xsi:type="boolean">false</item>
                   <item name="indexField" xsi:type="string">id</item>
               </item>
           </argument>
       </actionsColumn>
    </columns>
</listing>